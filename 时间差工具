#include<stdio.h>
int main()
{
    int hour1,minute1,hour2,minute2;
    printf("（计算时间差）请输入两个时间：\n");
    scanf("%d %d %d %d",&hour1,&minute1,&hour2,&minute2);
    int t1=hour1*60+minute1;
    int t2=hour2*60+minute2;
    int t3=t2-t1;
    if((t3/60)<0 || (t3%60)<0){
        int t4=-(t3/60),t5=-(t3%60);
        printf("时间差为%d时%d分。\n",t4,t5);
    }else{
    printf("时间差为%d时%d分。\n",t3/60,t3%60);
    }
    return 0;
}
